{
  "name": "liquidbcn/craftcms-rate-limit",
  "description": "Rate limiting abstraction which, in conjunction with your application's cache, provides an easy way to limit requests during a specified window of time.",
  "type": "craft-plugin",
  "keywords": ["craft", "cms", "craft-plugin", "rate limit"],
  "license": "proprietary",
  "authors": [
    {
      "name": "Marc Bernet",
      "homepage": "https://liquidbcn.com"
    }
  ],
  "support": {
    "email": "mbernet@liquid.cat",
    "issues": "https://github.com/LiquidBarcelona/craft-cms-rate-limit/issues?state=open",
    "source": "https://github.com/LiquidBarcelona/craft-cms-rate-limit",
    "docs": "https://github.com/LiquidBarcelona/craft-cms-rate-limit/blob/master/README.md"
  },
  "require": {
    "php": "^8.2",
    "craftcms/cms": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "liquidbcn\\ratelimit\\": "src/"
    }
  },
  "extra": {
    "name": "Rate Limit",
    "handle": "rate-limit",
    "class": "liquidbcn\\ratelimit\\RateLimit"
  }
}
